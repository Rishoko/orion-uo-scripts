function scavenge() {
    var list = Orion.FindList('sca', 'ground', '', 3)
    if (!list.length)
        return false;

    for (var i = 0; i < list.length; i++) {
        Orion.Wait(Orion.Random(800, 1200));
        Orion.MoveItem(list[i], 0, backpack);
    }
    return true;
}

function scavengeLoop() {
    while (true) {
        scavenge()
    }
}